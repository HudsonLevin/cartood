<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >

    <xacro:include filename="inertial_macros.xacro" /> 
    <material name="white"> 
        <color rgba="1 1 1 1" /> 
    </material>

    <material name="orange"> 
        <color rgba="1 0.3 0.1 1"/> 
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1"/> 
    </material>

    <material name="black"> 
        <color rgba="0 0 0 1"/> 
    </material>

    <material name="red"> 
        <color rgba="1 0 0 1"/> 
    </material>

<!-- BASE LINK -->

    <link name="base_link"> 

    </link>

    <link name="chassis"> 
        <visual> 
            <origin xyz="-0.011763 0.072599 0" rpy="0 0 0"/> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/cartood.dae" scale="1 1 1"/>
            </geometry>
            <material name="white"/> 
        </visual>
        <collision> 
            <origin xyz="-0.011763 0.072599 0" rpy="0 0 0"/> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/cartood.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="100" x="0" y="0" z="0"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_box>
    </link>
    <gazebo reference="chassis">
        <material>Gazebo/Orange</material>
    </gazebo>
    
    <joint name="chassis_joint" type="fixed"> 
        <parent link="base_link"/> 
        <child link="chassis"/> 
        <origin xyz="0 0 0"/> 
    </joint>

<!-- BASE_FOOTPRINT LINK -->

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="base_footprint">
    </link>

<!-- LEFT MIDDLE WHEEL -->

    <joint name="left_middle_wheel_joint" type="continuous"> 
        <parent link="base_link"/> 
        <child link="left_middle_wheel"/> 
        <origin xyz="0.162737 0.072599 0" rpy="0 0 0"/> 
        <axis xyz="1 0 0"/> 
        <dynamic dim="1" friction="100"/>
    </joint>
    <link name="left_middle_wheel"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="50" length="0.05" radius="0.05"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="left_middle_wheel">
        <material>Gazebo/Blue</material>
    </gazebo>

<!--LEFT BACK WHEEL-->

    <joint name="left_back_wheel_joint" type="continuous"> 
        <parent link="base_link"/> 
        <child link="left_back_wheel"/> 
        <origin xyz="0.162737 0.172599 0" rpy="0 0 0"/> 
        <axis xyz="1 0 0"/> 
        <dynamic dim="1" friction="100"/>
    </joint>
    <link name="left_back_wheel"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="50" length="0.05" radius="0.05"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="left_back_wheel">
        <material>Gazebo/Blue</material>
        <mu1>0.8</mu1>
    </gazebo>

<!--LEFT FRONT WHEEL-->

    <joint name="left_front_wheel_joint" type="continuous"> 
        <parent link="base_link"/> 
        <child link="left_front_wheel"/> 
        <origin xyz="0.162737 -0.027401 0" rpy="0 0 0"/> 
        <axis xyz="1 0 0"/>
        <dynamic dim="1" friction="100"/> 
    </joint>
    <link name="left_front_wheel"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="50" length="0.05" radius="0.05"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="left_front_wheel">
        <material>Gazebo/Blue</material>
    </gazebo>

<!-- RIGHT MIDDLE WHEEL -->

    <joint name="right_middle_wheel_joint" type="continuous"> 
        <parent link="base_link"/> 
        <child link="right_middle_wheel"/> 
        <origin xyz="-0.186263 0.072599 0" rpy="0 0 0"/> 
        <axis xyz="1 0 0"/> 
        <dynamic dim="1" friction="100"/>
    </joint>
    <link name="right_middle_wheel"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="50" length="0.05" radius="0.05"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="right_middle_wheel">
        <material>Gazebo/Blue</material>
    </gazebo>

<!-- RIGHT BACK WHEEL -->

    <joint name="right_back_wheel_joint" type="continuous"> 
        <parent link="base_link"/> 
        <child link="right_back_wheel"/> 
        <origin xyz="-0.186263 0.172599 0" rpy="0 0 0"/> 
        <axis xyz="1 0 0"/> 
        <dynamic dim="1" friction="100"/>
    </joint>
    <link name="right_back_wheel"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="50" length="0.05" radius="0.05"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="right_back_wheel">
        <material>Gazebo/Blue</material>
        
    </gazebo>

<!-- RIGHT FRONT WHEEL -->

    <joint name="right_front_wheel_joint" type="continuous">
        <parent link="base_link"/> 
        <child link="right_front_wheel"/> 
        <origin xyz="-0.186263 -0.027401 0" rpy="0 0 0"/> 
        <axis xyz="1 0 0"/> 
        <dynamic dim="1" friction="100"/>
    </joint>
    <link name="right_front_wheel"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/wheeltood.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="25" length="0.05" radius="0.05"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="right_front_wheel">
        <material>Gazebo/Blue</material>
    </gazebo>

<!--ARM UP AND DOWN -->

    <joint name="arm_up_down_joint" type="revolute"> 
        <parent link="chassis"/> 
        <child link="first_arm"/> 
        <origin xyz="-0.011763 -0.097401 0" rpy="0 0 0"/> 
        <axis xyz="-1 0 0"/> 
        <limit lower="0.0" upper="0.785" velocity="1" effort="100" />
    </joint>

    <link name="first_arm"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/joint1.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/joint1.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_box mass="0.1" x="0" y="0" z="0"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_box>
    </link>
    <gazebo reference="first_arm">
        <material>Gazebo/Blue</material>
        <mu1>1.4</mu1>
    </gazebo>

<!--ARM LEFT -->

    <joint name="double_arm_left" type="revolute"> 
        <parent link="first_arm"/> 
        <child link="left_arm"/> 
        <origin xyz="0.05 -0.07 0" rpy="-3.05093 1.57079 1.66145"/> 
        <axis xyz="1 0 0"/> 
        <limit lower="-0.785" upper="0.67" velocity="1" effort="100" />
    </joint>
    <link name="left_arm"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/arm.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/arm.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0" radius="0"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="left_arm">
        <material>Gazebo/Blue</material>
    </gazebo>

<!--ARM RIGHT -->

    <joint name="double_arm_right" type="revolute"> 
        <parent link="first_arm"/> 
        <child link="right_arm"/> 
        <origin xyz="-0.05 -0.07 0" rpy="-3.05093 -1.57079 -1.66145"/>
        <axis xyz="1 0 0"/> 
        <limit lower="-0.785" upper="0.67" velocity="1" effort="100" />
    </joint>
    <link name="right_arm"> 
        <visual>
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/arm.dae" scale="1 1 1"/>
            </geometry>
            <material name="blue"/> 
        </visual>
        <collision> 
            <geometry>
                <mesh filename="file://$(find articubot_one)/description/arm.dae" scale="1 1 1"/>
            </geometry>
        </collision>
        <xacro:inertial_cylinder mass="0.1" length="0" radius="0"> 
            <origin xyz="0 0 0" rpy="0 0 0"/> 
        </xacro:inertial_cylinder>
    </link>
    <gazebo reference="right_arm">
        <material>Gazebo/Blue</material>
    </gazebo>

</robot>